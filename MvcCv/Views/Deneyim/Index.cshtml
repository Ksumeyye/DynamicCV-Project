@using MvcCv.Models.Entity
@model List<TblDeneyimlerim>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br/>
<h2>Deneyim Listesi</h2>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>ID</th>
            <th>Şirket İsmi</th>
            <th>Unvan</th>
            <th>Tarih</th>
            <th>Sil</th>
            <th>Düzenle</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ID</td>
                <td>@item.Baslik</td>
                <td>@item.AltBaslik</td>
                <td>@item.Tarih</td>
                <td><a href="/Deneyim/DeneyimSil/@item.ID" class="btn btn-danger">Sil</a></td>
                <td><a href="/Deneyim/DeneyimGetir/@item.ID" class="btn btn-dark">Düzenle</a></td>
            </tr>
        }
    </tbody>
</table>
<a href="/Deneyim/DeneyimEkle/" class="btn btn-primary">Yeni Deneyim Ekle</a>